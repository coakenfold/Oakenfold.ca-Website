{
  "name": "oakenfold.ca",
  "version": "0.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run copy; bundle exec jekyll serve;",
    "build": "jekyll build; npm run inlineCss; npm run postcss; npm run copy; npm run htmlMinify; npm run jsMinify",
    "copy": "node buildScripts/copy.js",
    "htmlMinify": "node buildScripts/htmlMinify.js",
    "inlineCss": "node buildScripts/inlineCss.js",
    "jsMinify": "uglifyjs _site/script.js -o _site/script.js --compress",
    "inlineSource": "mv _site/index.html _site/index.original.html; cat _site/index.original.html | inline-source --compress false --root _site > _site/index.html",
    "postcss": "postcss _site/css --dir _site/css",
    "buildInline": "JEKYLL_ENV=production jekyll build; npm run postcss; npm run copy; npm run inlineSource; npm run htmlMinify; npm run jsMinify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coakenfold/Oakenfold.ca-Website.git"
  },
  "author": {
    "name": "Chad Oakenfold",
    "url": "http://oakenfold.ca/",
    "email": "web@oakenfold.ca"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coakenfold/Oakenfold.ca-Website/issues"
  },
  "homepage": "https://github.com/coakenfold/Oakenfold.ca-Website",
  "dependencies": {},
  "devDependencies": {
    "copy": "^0.3.2",
    "critical": "^1.3.2",
    "cssnano": "^3.10.0",
    "html-minifier": "^3.5.16",
    "http-server": "^0.11.1",
    "inline-source": "^6.1.5",
    "uglify": "^0.1.5"
  }
}
